      offsetY = touch.clientY - customerIcon.getBoundingClientRect().top;
      isDragging = true;
    }

    lastClickTime = currentTime;
  };

  const dragIcon = (e) => {
    e.preventDefault();
    if (isDragging) {
      const touch = e.type === 'touchmove' ? e.touches[0] : e;
      const newX = touch.clientX - offsetX;
      const newY = touch.clientY - offsetY;
      customerIcon.style.left = newX + 'px';
