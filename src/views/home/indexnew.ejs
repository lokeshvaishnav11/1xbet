/* =================================================================
   "ROYAL CASINO" THEME - UPDATED LOGIN MODAL CSS
   ================================================================= */

:root {
    --bg-color-dark: #1a0a2e; /* Deep Purple */
    --surface-color: #2a1a3e; /* Lighter Purple Surface */
    --primary-accent-gold: #FFD700; /* Bright Gold */
    --text-primary-light: #F0F0F0;
    --text-secondary-light: #bca8d1;
    --border-color-gold: rgba(255, 215, 0, 0.4);
    --glow-shadow-gold: 0 0 20px 0px rgba(255, 215, 0, 0.6);
}

/* --- Main Modal Container --- */
.l-modal .modal-dialog {
    max-width: 450px;
    margin: 1.75rem auto;
}
.l-modal .modal-content {
    background: linear-gradient(145deg, var(--surface-color), var(--bg-color-dark));
    border: 1px solid var(--border-color-gold);
    border-radius: 16px;
    color: var(--text-primary-light);
    box-shadow: 0 10px 40px rgba(0, 0, 0, 0.5);
    overflow: hidden;
}

/* --- Modal Header --- */
.login-header {
    background: rgba(0,0,0,0.2);
    border-bottom: 1px solid var(--border-color-gold);
    padding: 25px;
    justify-content: center;
    text-align: center;
}
.modal-header.login-header .modal-title {
    font-size: 1.8rem;
    font-weight: 700;
    color: var(--primary-accent-gold);
    text-transform: uppercase;
    letter-spacing: 2px;
    text-shadow: 0 0 10px var(--primary-accent-gold);
}
.login-header .material-symbols-outlined {
    display: none;
}

/* --- Redesigned Form Inputs with Full Borders --- */
.login-controls {
    background: rgba(0,0,0,0.3);
    position: relative;
    border: 1px solid var(--border-color-gold); /* Full border added */
    margin-bottom: 25px;
    border-radius: 8px; /* Rounded corners for the input field */
    transition: all 0.3s ease;
    display: flex;
    align-items: center;
}
.login-controls.input-focus {
    border-color: var(--primary-accent-gold);
    box-shadow: var(--glow-shadow-gold);
}

.login-controls label {
    border: none;
    display: flex;
    align-items: center;
    width: 100%;
    padding: 5px 0;
}
.login-controls label .input-ico {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    color: var(--text-secondary-light);
    font-size: 22px;
    transition: color 0.3s ease;
    left: 15px;
}
.login-controls.input-focus label .input-ico {
    color: var(--primary-accent-gold);
