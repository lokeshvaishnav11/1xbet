<script>
(function(){
  // safety: make sure DOM exists
  const track = document.querySelector(".slider-track");
  const images = track ? track.querySelectorAll("img") : [];
  const dotsContainer = document.querySelector(".slider-dots");
  const leftArrow = document.querySelector(".arrow-left");
  const rightArrow = document.querySelector(".arrow-right");

  if (!track || images.length === 0 || !dotsContainer) {
    console.error("Slider init failed: missing elements.", { track, imagesCount: images.length, dotsContainer });
    return;
  }

  let currentIndex = 0;
  const totalSlides = images.length;

  // Create dots and attach click handlers
  function createDots() {
    dotsContainer.innerHTML = "";
    for (let i = 0; i < totalSlides; i++) {
      const dot = document.createElement("span");
      dot.dataset.index = i;
      dot.setAttribute("role", "button");
      dot.setAttribute("aria-label", "Go to slide " + (i+1));
      if (i === 0) dot.classList.add("active");
      dot.addEventListener("click", onDotClick);
      dotsContainer.appendChild(dot);
    }
  }

  function onDotClick(e){
    const idx = parseInt(e.currentTarget.dataset.index, 10);
    goTo(idx);
  }

  function updateDots(idx) {
    const dots = dotsContainer.querySelectorAll("span");
    dots.forEach(d => d.classList.remove("active"));
    if (dots[idx]) dots[idx].classList.add("active");
  }

  function goTo(index) {
    if (index < 0) index = totalSlides - 1;
    if (index >= totalSlides) index = 0;
    currentIndex = index;
    // translate by index * 100%
    track.style.transform = 'translateX(-' + (index * 100) + '%)';
    updateDots(index);
  }

  // Arrow controls
  if (leftArrow && rightArrow) {
    leftArrow.addEventListener("click", () => goTo(currentIndex - 1));
    rightArrow.addEventListener("click", () => goTo(currentIndex + 1));
  }

  // Keyboard support
  window.addEventListener("keydown", function(e){
    if (e.key === "ArrowLeft") goTo(currentIndex - 1);
    if (e.key === "ArrowRight") goTo(currentIndex + 1);
  });

  // Ensure slides re-align when window resizes (just to be safe)
  window.addEventListener("resize", function(){
    // reapply transform so percentages still align (useful if layout changes)
    track.style.transform = 'translateX(-' + (currentIndex * 100) + '%)';
  });

  createDots();
  goTo(0);

  // Optional autoplay (comment out if you want only manual)
  let autoplay = true;
  let autoplayInterval = 4000;
  let autoplayId = null;

  function startAutoplay(){
    if (!autoplay) return;
    clearInterval(autoplayId);
    autoplayId = setInterval(() => {
      goTo((currentIndex + 1) % totalSlides);
    }, autoplayInterval);
  }
  function stopAutoplay(){ clearInterval(autoplayId); autoplayId = null; }

  // Pause autoplay while hovering
  const container = document.querySelector(".slider-container");
  container.addEventListener("mouseenter", stopAutoplay);
  container.addEventListener("mouseleave", startAutoplay);

  startAutoplay();

  console.info("Slider initialized. slides:", totalSlides);
})();
</script>
