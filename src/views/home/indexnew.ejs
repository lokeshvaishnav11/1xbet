  const customerIcon = document.getElementById('customerId');

  let lastClickTime = 0;
  let isDragging = false;
  let offsetX, offsetY;

  const handleClick = (e) => {
    const currentTime = new Date().getTime();
    const timeSinceLastClick = currentTime - lastClickTime;

    if (timeSinceLastClick < 250) { // Threshold for double-click
      // Double-click action
      window.location.href = 'https://telegram.me/jagaur_support_bot';
    } else {
